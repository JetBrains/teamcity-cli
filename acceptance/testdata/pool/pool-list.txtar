# Test pool list subcommand.

exec teamcity pool list --no-input
stdout '.'
! stderr 'Error'

# --json
exec teamcity pool list --json --no-input
stdout '"count"'
! stderr 'Error'

# --json=f1,f2 field selection accepts valid fields
exec teamcity pool list --json=id,name --no-input
stdout '"count"'
! stderr 'Error'
